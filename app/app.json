{
  "configVersion": "v3",
  "app": {
    "appId": 1058365,
    "appName": "Nightscout Companion App",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Nightscout data retriever for the watchface"
  },
  "permissions": [
    "device:os.bg_service", "data:os.device.info"   
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": [
            "page/index"
          ]
        },
        "app-service": {
          "services": ["app-service/index"]
        },        
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        } 
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 320
        }        
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Nightscout watchface companion app"
    }
  },
  "defaultLanguage": "en-US"
}
